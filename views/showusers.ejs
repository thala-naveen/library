  <!--Main Navigation-->
  
  <!--Main layout-->
  <main style="margin-top: 58px">
    <div class="container pt-4">
      <!-- Section: Main chart -->
      
      <!-- Section: Main chart -->
  
      <!--Section: Sales Performance KPIs-->
      <section class="mb-4">
        <div class="card">
          <div class="card-header text-center py-3">
            <h5 class="mb-0 text-center">
              <strong>Users</strong>
            </h5>
          </div>
          <div class="card-body">
            <div class="table-responsive">
              <table class="table table-hover text-nowrap" id="user-table">
                <thead>
                  <tr>
                    <th scope="col">user Email</th>
                    <th scope="col">User Enrollment</th>
                    <th scope="col">User Name</th>
                    <th scope="col">Course</th>
                    <th scope="col">Branch</th>
                    <th scope="col">Semester</th>
                    <th scope="col">Role</th>
                  </tr>
                </thead>
                <tbody>
                  <% data.map((item)=>{  %>
                    <tr>
                        <td scope="row"><%=item.mailid%></td>
                        <td scope="row"><%=item.firstname%></td>
                        <td scope="row"><%=item.lastname%></td>
                        <td scope="row"><%=item.course%></td>
                        <td scope="row"><%=item.branch%></td>
                        <td scope="row"><%=item.semester%></td>
                        <td scope="row"><%=item.role%></td>
                    </tr>
                    <% }) %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </section>
      
    </div>
  </main>
  
  <!--Main layout-->
